        <Setter Property="HeaderTemplate">
            <Setter.Value>
                <DataTemplate>
                    <Grid 
                        Margin="12,5,0,11"
                        VerticalAlignment="Stretch">

                        <TextBlock
                            x:Name="Header"
                            VerticalAlignment="Bottom"
                            Style="{StaticResource TitleTextBlockStyle}"
